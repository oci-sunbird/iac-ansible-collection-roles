service:
  type: ClusterIP

auth:
  adminUser: admin

ingress:
  enabled: false

replicaCount: 2

proxy: edge
production: true

resources:
  limits: {}
  requests:
    cpu: 200m
    memory: 1000Mi

rbac:
  create: true
  rules:
  - apiGroups:
    - ""
    resources:
    - pods
    verbs:
    - get
    - list

postgresql:
  enabled: false

externalDatabase:
  host: postgres-postgresql.postgres
  database: keycloakdb
  user: keycloakuser
  existingSecret: postgres-postgresql
  existingSecretPasswordKey: password
